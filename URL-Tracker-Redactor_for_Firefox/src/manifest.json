{
    "manifest_version": 2,
    "name": "ULR Tracker Redactor",
    "author": "Glovebox Team",
    "description": "Removes tracking information embedded in link. Made by Glovebox",
    "homepage_url": "https://github.com/lars0281/URL-Tracker-Redactor_for_Firefox",
    "version": "1.0.4",
    "icons": {
        "48": "icons/UTR-48.png",
        "96": "icons/UTR-96.png"
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "urltrackerredactor13@glovebox.com",
            "strict_min_version": "63.0"
        }
    },
    "browser_action": {
       "browser_style": true,
        "default_icon": "icons/UTR-96.png",
        "theme_icons": [{
                "light": "icons/UTR-32.png",
                "dark": "icons/UTR-32.png",
                "size": 32
            }
        ],
        "default_title":"URL Tracker Redactrr: To remove tracking information embedded in links."
    },

    "background": {
        "scripts": [
        	"utils/glovebox_utils.js",
           "background.js"
        ]
    },
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "activeTab",
        "tabs",
        "contextMenus",
        "menus",
		"storage",
        "unlimitedStorage",
        "<all_urls>"
    ]

}
